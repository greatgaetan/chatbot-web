n  border: 0;\r\n  font: inherit;\r\n  font-size: 100%;\r\n}\r\nul {\r\n  list-style: none;\r\n}\r\nli {\r\n  list-style: none;\r\n}\r\ninput,\r\ntextarea,\r\nselect,\r\nbutton {\r\n  display: block;\r\n  box-sizing: border-box;\r\n  border-radius: 0;\r\n  outline: none;\r\n  border: none;\r\n  background: none;\r\n  color: inherit;\r\n}\r\ninput:focus,\r\ntextarea:focus,\r\nbutton:focus {\r\n  outline: none;\r\n}\r\ninput[type=\"submit\"]:hover,\r\nbutton:hover {\r\n  cursor: pointer;\r\n}\r\ninput::-ms-clear {\r\n  display: none; /*─ IE11 → remove cross to close ─*/\r\n}\r\ntextarea {\r\n  overflow: auto; /*─ Hide scroll bar on IE ─*/\r\n}\r\na {\r\n  color: inherit;\r\n  text-decoration: none;\r\n}\r\nimg,\r\nvideo,\r\nsvg,\r\npicture {\r\n  display: block;\r\n}\r\nimg,\r\nvideo {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\nsvg {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\niframe {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n","css\\styles.scss":"@import \"reset.scss\";\r\n\r\n$black-text: #202226;\r\n$light-grey: #f6f8fc;\r\n$grey: #f7f7f7;\r\n$blue: #81b4ec;\r\n\r\n* {\r\n  font-family: \"Euclid Circular A\", sans-serif;\r\n  color: $black-text;\r\n}\r\n\r\nbody {\r\n  overflow-y: scroll;\r\n}\r\n\r\n.semibold {\r\n  font-weight: 600;\r\n}\r\n\r\n#img-embedded-text {\r\n  max-width: 50px;\r\n  height: auto;\r\n  display: inline;\r\n  position: relative;\r\n  top: 8px;\r\n}\r\n\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─\r\n/*─                                                                                  TRANSITION\r\n/*─\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n\r\n.transition-fade {\r\n  opacity: 1;\r\n  transition: 500ms;\r\n}\r\n\r\nhtml.is-animating .transition-fade {\r\n  opacity: 0;\r\n}\r\n\r\n.delay1 {\r\n  transition-delay: 0.2s;\r\n}\r\n\r\n.delay2 {\r\n  transition-delay: 0.4s;\r\n}\r\n\r\n.delay3 {\r\n  transition-delay: 0.6s;\r\n}\r\n\r\nhtml.is-leaving .transition-fade {\r\n  transition-delay: 0s !important;\r\n}\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─\r\n/*─                                                                                      NAVBAR\r\n/*─\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n\r\n#hamburgerButton {\r\n  display: none;\r\n}\r\n\r\n#navbar {\r\n  display: inline-flex;\r\n  width: 100%;\r\n  justify-content: right;\r\n  padding: 30px 0;\r\n  li {\r\n    margin-right: 40px;\r\n    font-size: 1.5rem;\r\n    font-weight: 300;\r\n\r\n    a {\r\n      display: block;\r\n      padding: 15px;\r\n      text-decoration: none;\r\n      color: #aaa;\r\n      font-weight: 800;\r\n      text-transform: uppercase;\r\n      margin: 0 10px;\r\n    }\r\n\r\n    a,\r\n    a:after,\r\n    a:before {\r\n      transition: all 0.5s;\r\n    }\r\n\r\n    a:hover {\r\n      color: $black-text;\r\n    }\r\n  }\r\n}\r\n\r\n/* SHIFT */\r\nnav.shift ul li a {\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\nnav.shift ul li a:after {\r\n  display: block;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n  margin: auto;\r\n  width: 100%;\r\n  height: 1px;\r\n  content: \".\";\r\n  color: transparent;\r\n  // original color: background: #f1c40f;\r\n  background: $blue;\r\n  visibility: none;\r\n  opacity: 0;\r\n  z-index: -1;\r\n}\r\nnav.shift ul li a:hover:after {\r\n  opacity: 1;\r\n  visibility: visible;\r\n  height: 100%;\r\n}\r\n\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─\r\n/*─                                                                                       FOOTER\r\n/*─\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n\r\nfooter {\r\n  background: $blue;\r\n  text-align: center;\r\n  padding: 30px 0px;\r\n}\r\n\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─\r\n/*─                                                                  HOMEPAGE BLOCK INTRODUCTION\r\n/*─\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n/*─────────────────────────────────────────────────────────────────────────────────────────────────*/\r\n#homepage-content {\r\n  width: 100%;\r\n}\r\n#homepage-block {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-template-rows: 1fr;\r\n  grid-column-gap: 0px;\r\n  grid-row-gap: 0px;\r\n  background: $light-grey;\r\n  border-radius: 20px;\r\n  max-width: 80%;\r\n  padding: 40px;\r\n  margin: 20px auto;\r\n  align-items: center;\r\n\r\n  #left {\r\n    grid-area: 1 / 1 / 2 / 3;\r\n    p {\r\n      text-align: center;\r\n      font-weight: 400;\r\n      font-size: 1.7rem;\r\n    }\r\n  }\r\n\r\n  #right {\r\n    grid-area: 1 / 3 / 2 / 4;\r\n  }\r\n}\r\n\r\n#myself-block {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-template-rows: 1fr;\r\n  grid-column-gap: 0px;\r\n  grid-row-